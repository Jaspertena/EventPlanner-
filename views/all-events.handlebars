<div class="events">
    {{#each event}}
    <div class="card">
        <div class="card-header">
            <h2>
                <a href="/post/{{id}}">{{title}}</a>
            </h2>
        </div>
        <div class="card-body">
          {{ body }}
        </div>
    </div>
    {{/each}}
</div>