
<div class="card-header">
    <h2>Add New Event</h2>
  </div>
<div class="events">
    <div id="newevent" role="tabpanel">
    <form id="newevent-form" class="card-body">
      <div>
        <label for="name-input-signup">* Event Name</label>
        <input type="text" id="name-input-signup" />
      </div>
      <div>
        <label for="description-input-signup">Event Description</label>
        <input type="description" id="description-input-signup" />
      </div>
      <div>
        <label for="event_start-input-signup">* Event Start Time</label>
        <input type="event_start" id="event_start-input-signup" />
      </div>
      <div>
        <label for="event_end-input-signup">Event End Time</label>
        <input type="event_end" id="start_end-input-signup" />
      </div>
      <div>
        <label for="location-input-signup">* Location</label>
        <input type="location" id="location-input-signup" />
      </div>
      <div>
        <label for="cost-input-signup">Cost</label>
        <input type="cost" id="cost-input-signup" />
      </div>
      <div>
        <label for="link-input-signup">Website Link</label>
        <input type="link" id="link-input-signup" />
      </div>
    
    <button type="submit" id="new_event-btn" class="btn" >Add Event, Have FUN!</button>
      <a href="/login">Login instead</a>
      <p>* Required</p>
    </form>
  </div>
</div>

<script src="/js/newEvent.js"></script>


<div class="card">
  <div class="card-header">
    <h2>Create New Event</h2>
  </div>
  <form id="new-post-form" class="card-body">
    <label class="form-label" for="event-title">Title</label>
    <input type="text" name="event-title" class="form-input" />

    <label class="form-label" for="post-body">Content</label>
    <textarea name="post-body" class="form-input"></textarea>

    <button type="submit" class="btn">Create</button>
  </form>
</div>

<script src="/js/new.js"></script>

